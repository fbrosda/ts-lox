{
  "*": {
    "console": "node",
    "start": "make run",
    "make": "make build"
  },
  "src/*.ts": {
    "type": "source",
    "dispatch": "npx eslint --format compact --fix %"
  },
  "index.ts": {
    "type": "source",
    "dispatch": "npx eslint --format compact --fix %"
  },
  "*.json": {
    "type": "config",
    "dispatch": "npx prettier --write %"
  }
}
